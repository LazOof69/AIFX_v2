# Environment
NODE_ENV=development

# Server
PORT=3000

# Database
DATABASE_URL=postgresql://username:password@localhost:5432/aifx_v2

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=your-jwt-secret-key-change-this-in-production
JWT_REFRESH_SECRET=your-jwt-refresh-secret-key-change-this-in-production
JWT_EXPIRES_IN=1h
JWT_REFRESH_EXPIRES_IN=7d

# API Keys
ALPHA_VANTAGE_KEY=your-alpha-vantage-api-key
TWELVE_DATA_KEY=your-twelve-data-api-key

# Discord Bot
DISCORD_BOT_TOKEN=your-discord-bot-token

# ML Engine
ML_API_URL=http://localhost:8000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info

# Admin Dashboard
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your-secure-admin-password
ADMIN_JWT_SECRET=your-admin-jwt-secret-key