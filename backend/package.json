{
  "name": "aifx-v2-backend",
  "version": "1.0.0",
  "description": "AI-powered forex trading advisory system backend",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "migrate:undo:all": "sequelize-cli db:migrate:undo:all",
    "migrate:status": "sequelize-cli db:migrate:status",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo",
    "seed:undo:all": "sequelize-cli db:seed:undo:all",
    "db:reset": "npm run migrate:undo:all && npm run migrate && npm run seed",
    "db:setup": "npm run migrate && npm run seed",
    "test:auth": "node test-auth.js",
    "test:forex": "node test-forex.js",
    "test:forex:quick": "node test-forex.js --quick",
    "test:all": "npm run test:auth && npm run test:forex"
  },
  "keywords": [
    "forex",
    "trading",
    "ai",
    "api",
    "signals"
  ],
  "author": "AIFX Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.0.0",
    "bcrypt": "^5.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.0",
    "dotenv": "^16.0.0",
    "express": "^4.18.0",
    "express-rate-limit": "^6.0.0",
    "helmet": "^7.0.0",
    "joi": "^17.0.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "pg-hstore": "^2.3.4",
    "redis": "^4.0.0",
    "sequelize": "^6.0.0",
    "socket.io": "^4.0.0",
    "technicalindicators": "^3.1.0",
    "uuid": "^9.0.0",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.0",
    "babel-jest": "^29.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "sequelize-cli": "^6.6.3",
    "supertest": "^6.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
