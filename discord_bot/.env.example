NODE_ENV=development

# Rate Limiting
RATE_LIMIT_MAX_NOTIFICATIONS_PER_MINUTE=1
RATE_LIMIT_MAX_COMMANDS_PER_MINUTE=5

# Logging
LOG_LEVEL=info